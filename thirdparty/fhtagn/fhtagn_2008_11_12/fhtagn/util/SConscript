Import('env')

SOURCES = []

HEADERS = []

if env.has_key('FHTAGN_CPPUNIT_VERSION'):
  SOURCES += [
    'cppunit_output.cpp',
  ]

  HEADERS += [
    'cppunit_output.h',
  ]


env.addSources('fhtagn_util', SOURCES)
env.addHeaders('fhtagn_util', HEADERS)
